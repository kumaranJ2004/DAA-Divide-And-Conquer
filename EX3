#include <stdio.h>

int main() {
    int n, x;
    scanf("%d", &n);
    int arr[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    scanf("%d", &x);

    int low = 0, high = n - 1, mid, floor = -1;

    while (low <= high) {
        mid = (low + high) / 2;
        if (arr[mid] == x) {
            floor = arr[mid];
            break;
        }
        if (arr[mid] < x) {
            floor = arr[mid];
            low = mid + 1;
        } else {
            high = mid - 1;
        }
    }

    printf("%d\n", floor);
    return 0;
}
